{
  "expo": {
    "name": "Runa Finance",
    "slug": "runa-finance",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.runafinance.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.runafinance.app"
    },
    "plugins": [
      "expo-router",
      "expo-asset"
    ],
    "scheme": "runa-finance"
  }
}
