{
  "expo": {
    "name": "Runa Finance",
    "slug": "runa-finance",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#788FAC"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.runafinance.app",
      "statusBar": {
        "style": "light",
        "backgroundColor": "#788FAC"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#788FAC"
      },
      "package": "com.runafinance.app",
      "navigationBar": {
        "backgroundColor": "#788FAC"
      },
      "statusBar": {
        "backgroundColor": "#788FAC",
        "barStyle": "light-content",
        "translucent": false
      }
    },
    "plugins": [
      "expo-router",
      "expo-asset"
    ],
    "scheme": "runa-finance"
  }
}
